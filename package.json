{
  "name": "nanami",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build ",
    "start": "next start",
    "lint": "eslint src --ext=ts",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "seed:db": "ts-node --project tsconfig.db.json lib/seed.ts",
    "clean:db": "ts-node --project tsconfig.db.json lib/clean.ts",
    "prisma:update": "prisma generate",
    "prisma:studio": "prisma studio",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:deploy": "vitest --run",
    "test:snapshot": "vitest -u"
  },
  "lint-staged": {
    "src/**/*.+(ts|tsx)": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@developerskyi/react-components": "^2.1.0",
    "@faker-js/faker": "^8.4.1",
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.17.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-toast": "^1.1.5",
    "@testing-library/jest-dom": "^6.3.0",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vercel/postgres": "^0.8.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "eslint": "8.56.0",
    "next": "^14.2.1",
    "next-auth": "^4.24.7",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "tailwind-variants": "^0.1.20",
    "usehooks-ts": "^2.10.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@iconify/react": "^4.1.1",
    "@next/eslint-plugin-next": "^14.1.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.2.2",
    "autoprefixer": "^10.4.17",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^9.0.7",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "postcss-import": "^16.0.0",
    "prettier": "^3.2.4",
    "prisma": "^5.17.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  }
}
